{
  "name": "@atomiton/store",
  "version": "0.1.1",
  "description": "State management for Atomiton workflow automation",
  "type": "module",
  "private": true,
  "imports": {
    "#*": "./src/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest run src/integration"
  },
  "keywords": [
    "store",
    "state-management",
    "zustand",
    "workflow",
    "automation"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "dependencies": {
    "@atomiton/utils": "workspace:*",
    "immer": "^10.0.0",
    "zustand": "^5.0.2"
  },
  "peerDependencies": {
    "react": "catalog:"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@redux-devtools/extension": "^3.3.0",
    "react": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
