{
  "name": "@atomiton/conductor",
  "version": "0.2.0",
  "description": "Blueprint and node execution orchestrator for Atomiton",
  "type": "module",
  "private": true,
  "exports": {
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    },
    "./desktop": {
      "import": "./dist/desktop.js",
      "types": "./dist/desktop.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --exclude src/__benchmarks__/**",
    "test:unit": "vitest run --exclude src/__benchmarks__/**",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:smoke": "vitest run src/**/*.smoke.test.ts --passWithNoTests",
    "test:benchmark": "vitest bench --run",
    "test:e2e": "echo \"No E2E tests for this package\"",
    "test:all": "pnpm test:unit && pnpm test:smoke && pnpm test:benchmark && pnpm test:e2e"
  },
  "dependencies": {
    "@atomiton/events": "workspace:*",
    "@atomiton/nodes": "workspace:*",
    "@atomiton/storage": "workspace:*",
    "@atomiton/store": "workspace:*",
    "@atomiton/utils": "workspace:*",
    "p-queue": "^8.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@types/node": "catalog:",
    "electron": "^38.0.0",
    "eslint": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "conductor",
    "orchestration",
    "execution",
    "workflow",
    "blueprint"
  ],
  "author": "Atomiton",
  "license": "MIT"
}
