{
  "name": "@atomiton/storage",
  "version": "0.0.1",
  "description": "Universal storage abstraction for Composites and application data across platforms",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "kill": "echo 'No processes to kill for Storage package'",
    "test:unit": "vitest run",
    "test:smoke": "vitest run src/**/*.smoke.test.{ts,tsx} --passWithNoTests || echo \"No smoke tests yet\"",
    "test:benchmark": "vitest bench --run",
    "test:e2e": "echo \"No E2E tests for this package\"",
    "test:all": "pnpm test:unit && pnpm test:smoke && pnpm test:benchmark && pnpm test:e2e",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@atomiton/nodes": "workspace:*"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "storage",
    "abstraction",
    "filesystem",
    "cloud",
    "composite",
    "cross-platform"
  ],
  "author": "Atomiton",
  "license": "MIT"
}
