{
  "name": "@atomiton/storage",
  "version": "0.2.0",
  "description": "Universal storage abstraction for Composites and application data across platforms",
  "type": "module",
  "private": true,
  "imports": {
    "#*": "./src/*"
  },  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/exports/browser/index.js",
      "types": "./dist/exports/browser/index.d.ts"
    },
    "./desktop": {
      "import": "./dist/exports/desktop/index.js",
      "types": "./dist/exports/desktop/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@atomiton/nodes": "workspace:*",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "storage",
    "abstraction",
    "filesystem",
    "cloud",
    "composite",
    "cross-platform"
  ],
  "author": "Atomiton",
  "license": "MIT"
}
