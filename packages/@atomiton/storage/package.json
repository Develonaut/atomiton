{
  "name": "@atomiton/storage",
  "version": "0.0.1",
  "description": "Universal storage abstraction for Blueprints and application data across platforms",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "kill": "echo 'No processes to kill for Storage package'"
  },
  "dependencies": {
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:^",
    "@atomiton/typescript-config": "workspace:^",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "storage",
    "abstraction",
    "filesystem",
    "cloud",
    "blueprint",
    "cross-platform"
  ],
  "license": "MIT"
}