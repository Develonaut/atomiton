{
  "name": "@atomiton/yaml",
  "version": "0.1.0",
  "description": "YAML parsing utilities for Atomiton Flow system",
  "type": "module",
  "private": true,
  "imports": {
    "#*": "./src/*"
  },  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "yaml",
    "parser",
    "flow",
    "automation"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@atomiton/flow": "workspace:*",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
