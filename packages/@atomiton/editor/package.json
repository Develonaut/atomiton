{
  "name": "@atomiton/editor",
  "version": "0.1.0",
  "description": "Atomiton visual editor",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/editor.css",
    "./*.css": "./src/*.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "dev:serve": "vite",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "keywords": [
    "editor",
    "visual",
    "workflow",
    "react-flow"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "dependencies": {
    "@atomiton/hooks": "workspace:*",
    "@atomiton/nodes": "workspace:*",
    "@atomiton/store": "workspace:*",
    "@atomiton/ui": "workspace:*",
    "@atomiton/utils": "workspace:*",
    "@dagrejs/dagre": "^1.1.5",
    "@xyflow/react": "^12.0.0",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@types/dagre": "^0.7.53",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  }
}
