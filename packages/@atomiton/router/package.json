{
  "name": "@atomiton/router",
  "version": "0.1.0",
  "description": "Domain-agnostic router package with auto-generated navigation methods and TanStack Router integration",
  "type": "module",
  "private": true,
  "imports": {
    "#*": "./src/*"
  },  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:benchmark": "vitest bench --run"
  },
  "keywords": [
    "router",
    "navigation",
    "tanstack",
    "react",
    "typescript"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "dependencies": {
    "@atomiton/store": "workspace:*",
    "@atomiton/utils": "workspace:*",
    "@tanstack/react-router": "^1.62.2",
    "path-to-regexp": "^8.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
