{
  "name": "@atomiton/eslint-config",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "default": "./dist/base.js"
    },
    "./react-internal": {
      "types": "./dist/react-internal.d.ts",
      "default": "./dist/react-internal.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "echo 'No formatting needed for config-only package'",
    "format:check": "echo 'No format check needed for config-only package'",
    "knip": "echo 'No dead code detection needed for config-only package'",
    "knip:fix": "echo 'No dead code fixes needed for config-only package'",
    "lint": "echo 'Linting eslint-config itself'",
    "lint:fix": "echo 'Linting eslint-config itself with fix'",
    "test": "echo 'No tests for config-only package'",
    "test:watch": "echo 'No test watch for config-only package'",
    "test:coverage": "echo 'No test coverage for config-only package'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@atomiton/typescript-config": "workspace:*",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-turbo": "^2.5.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
