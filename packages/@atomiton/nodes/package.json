{
  "name": "@atomiton/nodes",
  "version": "0.2.0",
  "description": "Atomiton Blueprint node implementations",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "exports": {
    "./browser": {
      "import": "./dist/exports/browser/index.js",
      "types": "./dist/exports/browser/index.d.ts"
    },
    "./executable": {
      "import": "./dist/exports/executable/index.js",
      "types": "./dist/exports/executable/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "pnpm test:unit",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:smoke": "mkdir -p test-results && vitest run 'smoke' --passWithNoTests --reporter=json > test-results/speed-report.json || echo \"No smoke tests yet\"",
    "test:benchmark": "vitest bench --run",
    "test:e2e": "echo \"No E2E tests for this package\"",
    "test:all": "pnpm test:unit && pnpm test:smoke && pnpm test:benchmark && pnpm test:e2e"
  },
  "keywords": [
    "nodes",
    "blueprint",
    "automation",
    "workflow"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "dependencies": {
    "@atomiton/store": "workspace:*",
    "@atomiton/utils": "workspace:*",
    "@atomiton/validation": "workspace:*",
    "@atomiton/yaml": "workspace:*"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@types/glob": "^9.0.0",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "glob": "^11.0.3",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
