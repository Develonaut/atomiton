{
  "name": "@atomiton/nodes",
  "version": "0.4.0",
  "description": "Atomiton Blueprint node implementations",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    "./definitions": {
      "import": "./dist/definitions.js",
      "types": "./dist/definitions.d.ts"
    },
    "./executables": {
      "import": "./dist/executables.js",
      "types": "./dist/executables.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:integration": "vitest run src/integration"
  },
  "keywords": [
    "nodes",
    "blueprint",
    "automation",
    "workflow"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "dependencies": {
    "@atomiton/store": "workspace:*",
    "@atomiton/utils": "workspace:*",
    "@atomiton/validation": "workspace:*",
    "@atomiton/yaml": "workspace:*",
    "@types/js-yaml": "^4.0.9",
    "isolated-vm": "^6.0.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@atomiton/vite-config": "workspace:*",
    "@types/glob": "^9.0.0",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "glob": "^11.0.3",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "catalog:"
  }
}
