{
  "name": "@atomiton/nodes",
  "version": "0.1.0",
  "description": "Atomiton Blueprint node implementations",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest run",
    "test:smoke": "vitest run src/**/*.smoke.test.{ts,tsx} --passWithNoTests || echo \"No smoke tests yet\"",
    "test:benchmark": "vitest bench --run",
    "test:e2e": "echo \"No E2E tests for this package\"",
    "test:all": "pnpm test:unit && pnpm test:smoke && pnpm test:benchmark && pnpm test:e2e",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "nodes",
    "blueprint",
    "automation",
    "workflow"
  ],
  "author": "Atomiton",
  "license": "MIT",
  "dependencies": {
    "@atomiton/store": "workspace:*",
    "@atomiton/utils": "workspace:*",
    "@atomiton/yaml": "workspace:*",
    "zod": "^3.25.3"
  },
  "devDependencies": {
    "@atomiton/eslint-config": "workspace:*",
    "@atomiton/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
